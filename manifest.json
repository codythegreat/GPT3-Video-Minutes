{

  "manifest_version": 2,
  "name": "GPT3 Video Minutes",
  "version": "1.0",
  "permissions": [
    "<all_urls>"
  ],

  "description": "Generates summary information for each minute of a YouTube video.",

  "icons": {
    "48": "icons/robot-head.svg"
  },

  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/watch*"],
      "js": ["gpt3VideoMinutes.js"],
      "css": ["style.css"]
    }
  ]

}
